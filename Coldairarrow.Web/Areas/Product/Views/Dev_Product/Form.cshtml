@using Coldairarrow.Entity.Product;
@using Coldairarrow.Util;

@{
    Layout = "~/Views/Shared/_Layout_Form.cshtml";

    var obj = (Dev_Product)Model;
    var objStr = Html.Raw(obj.ToJson());
}
<div style="padding:15px;padding-right:45px;">
    <form id="form" class="form-horizontal" role="form">
        
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">商讯标题</label>
            <div class="col-sm-5">
                <input name="ProductTitle" value="@obj.ProductTitle" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">商讯主图</label>
            <div class="col-sm-5">
                <input name="ProductPic" value="@obj.ProductPic" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">商讯摘要</label>
            <div class="col-sm-5">
                <input name="ProductRemark" value="@obj.ProductRemark" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">商讯内容</label>
            <div class="col-sm-5">
                <input name="ProductContent" value="@obj.ProductContent" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">所属商号</label>
            <div class="col-sm-5">
                <input name="ShopId" value="@obj.ShopId" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">创建人</label>
            <div class="col-sm-5">
                <input name="UserId" value="@obj.UserId" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">所属类型</label>
            <div class="col-sm-5">
                <input name="TypeId" value="@obj.TypeId" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">所属行业</label>
            <div class="col-sm-5">
                <input name="ClassId" value="@obj.ClassId" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">所属应用</label>
            <div class="col-sm-5">
                <input name="ChannelId" value="@obj.ChannelId" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">省份</label>
            <div class="col-sm-5">
                <input name="Provice" value="@obj.Provice" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">城市</label>
            <div class="col-sm-5">
                <input name="City" value="@obj.City" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">区县</label>
            <div class="col-sm-5">
                <input name="Area" value="@obj.Area" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">收费设置</label>
            <div class="col-sm-5">
                <input name="FeeWay" value="@obj.FeeWay" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">费用</label>
            <div class="col-sm-5">
                <input name="PayFee" value="@obj.PayFee" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">来源</label>
            <div class="col-sm-5">
                <input name="FromWay" value="@obj.FromWay" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">作者</label>
            <div class="col-sm-5">
                <input name="Author" value="@obj.Author" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">出处</label>
            <div class="col-sm-5">
                <input name="Source" value="@obj.Source" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">是否推广</label>
            <div class="col-sm-5">
                <input name="IsAd" value="@obj.IsAd" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">推广支出</label>
            <div class="col-sm-5">
                <input name="AdFee" value="@obj.AdFee" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">评论数量</label>
            <div class="col-sm-5">
                <input name="CommentNum" value="@obj.CommentNum" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">浏览数量</label>
            <div class="col-sm-5">
                <input name="ClickNum" value="@obj.ClickNum" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">收藏数量</label>
            <div class="col-sm-5">
                <input name="CollectionNum" value="@obj.CollectionNum" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">点赞数量</label>
            <div class="col-sm-5">
                <input name="GoodNum" value="@obj.GoodNum" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">收费累计</label>
            <div class="col-sm-5">
                <input name="GetPayFee" value="@obj.GetPayFee" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">打赏累计</label>
            <div class="col-sm-5">
                <input name="GetRewardFee" value="@obj.GetRewardFee" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">创建时间</label>
            <div class="col-sm-5">
                <input name="CreateTime" value="@obj.CreateTime" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">状态</label>
            <div class="col-sm-5">
                <input name="Status" value="@obj.Status" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">操作时间</label>
            <div class="col-sm-5">
                <input name="StatusTime" value="@obj.StatusTime" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-2 control-label">排序</label>
            <div class="col-sm-5">
                <input name="Sort" value="@obj.Sort" type="text" class="form-control" required>
                <div class="help-block with-errors"></div>
            </div>
        </div>
        <div class="form-group">
            <button id="submit" type="submit" class="hidden">提交</button>
        </div>
    </form>
</div>
<script>
    var theEntity = @objStr;

    $(function () {
        initEvent();
    });

    //事件绑定
    function initEvent() {
        //表单校验
        $('#form').validator().on('submit', function (e) {
            //校验成功
            if (!e.isDefaultPrevented()) {
                e.preventDefault();

                var values = $('#form').getValues();
                
                $.extend(theEntity, values);
                loading();
                $.postJSON('/Product/Dev_Product/SaveData', theEntity, function (resJson) {
                    loading(false);

                    if (resJson.Success) {
                        parent.$('#dataTable').bootstrapTable('refresh');
                        parent.dialogSuccess();
                        dialogClose();
                    }
                    else {
                        dialogError(resJson.Msg);
                    }
                });
            }
        })
    }

    //提交表单
    function submitForm() {
        $('#submit').trigger('click');
    }
</script>